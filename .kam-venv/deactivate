#!/bin/sh
# Kam Virtual Environment Deactivation Script

if [ -n "${KAM_VENV_ACTIVE:-}" ]; then
    # Call the deactivate function if it exists
    if type deactivate > /dev/null 2>&1; then
        deactivate
    else
        echo "No active Kam virtual environment found"
    fi
else
    echo "No active Kam virtual environment found"
fi
