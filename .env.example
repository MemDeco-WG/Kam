# Environment variables for Kam testing
# Copy this file to .env and modify as needed

# MMRL repository URL for testing
MMRL_REPO_URL=https://example.com/modules

# Device model for testing
DEVICE_MODEL=SM-A705FN

# CPU architecture
CPU_ABI=arm64-v8a

# Root manager (magisk, kernelsu, apatch)
ROOT_MANAGER=magisk

# Enable debug logging
DEBUG=true

# Local repository path
LOCAL_REPO_PATH=./local_modules
